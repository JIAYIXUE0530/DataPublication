function sumNumbers(num1: number, num2:number) {
    return num1 + num2
}

function sumNumbers2(num1: number, num2:number): number | string {
    const sum = num1 + num2;
    
    return sum.toString();
}


function sumNumbers3(num1: number, num2:number, num3?: number): number {
    if(num3 !== undefined)
        return num1 + num2 + num3;
    else
        return num1 + num2;
}